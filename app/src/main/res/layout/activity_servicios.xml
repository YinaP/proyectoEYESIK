<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondoeyesik"
    tools:context=".servicios">

    <ImageView
        android:id="@+id/maquillaje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="22dp"
        app:layout_constraintStart_toStartOf="@+id/boton8"
        app:layout_constraintTop_toBottomOf="@+id/tv14"
        app:srcCompat="@drawable/maquillaje"
        android:contentDescription="TODO" />

    <ImageView
        android:id="@+id/facial"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="29dp"
        android:layout_marginTop="22dp"
        app:layout_constraintStart_toStartOf="@+id/boton9"
        app:layout_constraintTop_toBottomOf="@+id/tv14"
        app:srcCompat="@drawable/facial"
        android:contentDescription="TODO" />

    <Button
        android:id="@+id/boton8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="29dp"
        android:backgroundTint="@color/DarkTurquoise"
        android:text="maquillaje"
        app:cornerRadius="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/maquillaje" />

    <Button
        android:id="@+id/boton9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:layout_marginEnd="25dp"
        android:backgroundTint="@color/DarkTurquoise"
        android:text="servicio facial"
        app:cornerRadius="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/facial" />

    <ImageView
        android:id="@+id/peluqueria"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginBottom="29dp"
        app:layout_constraintBottom_toTopOf="@+id/boton10"
        app:layout_constraintStart_toStartOf="@+id/boton10"
        app:srcCompat="@drawable/peluqueria" />

    <ImageView
        android:id="@+id/masaje"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="23dp"
        app:layout_constraintBottom_toTopOf="@+id/boton11"
        app:layout_constraintStart_toStartOf="@+id/boton11"
        app:srcCompat="@drawable/masaje" />

    <Button
        android:id="@+id/boton10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginBottom="33dp"
        android:backgroundTint="@color/DarkTurquoise"
        android:text="peluqueria"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toTopOf="@+id/manicure"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/boton11"
        android:layout_width="133dp"
        android:layout_height="49dp"
        android:layout_marginTop="130dp"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="131dp"
        android:backgroundTint="@color/DarkTurquoise"
        android:text="masajes"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/manicure"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/peluqueria" />

    <ImageView
        android:id="@+id/manicure"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginBottom="28dp"
        app:layout_constraintBottom_toTopOf="@+id/boton12"
        app:layout_constraintStart_toStartOf="@+id/boton12"
        app:srcCompat="@drawable/manicura" />

    <ImageView
        android:id="@+id/depilacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:layout_marginBottom="27dp"
        app:layout_constraintBottom_toTopOf="@+id/boton13"
        app:layout_constraintStart_toStartOf="@+id/boton13"
        app:srcCompat="@drawable/depilacion" />

    <Button
        android:id="@+id/boton12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="49dp"
        android:backgroundTint="@color/DarkTurquoise"
        android:text="manicure_pedicure"
        app:cornerRadius="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/boton13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:backgroundTint="@color/DarkTurquoise"
        android:text="depilacion"
        app:cornerRadius="20dp"
        app:layout_constraintBaseline_toBaselineOf="@+id/boton12"
        app:layout_constraintStart_toEndOf="@+id/boton12" />

    <TextView
        android:id="@+id/tv14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="31dp"
        android:text="SERVICIOS"
        android:textColor="@color/DarkTurquoise"
        android:textSize="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>