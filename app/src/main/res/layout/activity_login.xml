<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondoeyesik"
    tools:context=".login">

    <TextView
        android:id="@+id/tv8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="17dp"
        android:text="E-mail"
        android:textColor="@color/DarkTurquoise"
        android:textSize="28sp"
        app:layout_constraintBottom_toTopOf="@+id/et6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="33dp"
        android:layout_marginTop="12dp"
        android:text="Contrase単a"
        android:textColor="@color/DarkTurquoise"
        android:textSize="28sp"
        app:layout_constraintStart_toStartOf="@+id/et6"
        app:layout_constraintTop_toBottomOf="@+id/et6" />

    <EditText
        android:id="@+id/et6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="232dp"
        android:ems="10"
        android:hint="Ingrese su E-mail"
        android:inputType="textEmailAddress"
        app:layout_constraintStart_toStartOf="@+id/et7"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/et7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:hint="Ingrese su contrase単a"
        android:inputType="textPassword"
        app:layout_constraintStart_toStartOf="@+id/boton4"
        app:layout_constraintTop_toBottomOf="@+id/tv9" />

    <Button
        android:id="@+id/boton4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:backgroundTint="@color/DarkTurquoise"
        android:text="多Olvidaste tu contrase単a?"
        app:cornerRadius="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et7" />

    <Button
        android:id="@+id/boton5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:backgroundTint="@color/DarkTurquoise"
        android:text="login"
        app:cornerRadius="20dp"
        app:layout_constraintEnd_toEndOf="@+id/boton4"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="@+id/boton4"
        app:layout_constraintTop_toBottomOf="@+id/boton4" />

</androidx.constraintlayout.widget.ConstraintLayout>